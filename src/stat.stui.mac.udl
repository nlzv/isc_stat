 if $D(^rOBJ("%zreadline")) {
   do get^%zreadline("WebGW log>",.MsPerfConfigFile,.MsPerfConfigFileOffset,.InsertMode,"FS")
 }
 else {
   R "msperfmon config file>",MsPerfConfigFile W !
 }
 set @CtlGlo@("msperf","config")=$G(MsPerfConfigFile)
 quit
