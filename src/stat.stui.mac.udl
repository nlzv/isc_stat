WebGWSave
 if $G(itemid)="webgw"
 if %webgwname="" W "name can not be empty string",! quit
 set webgwrec=$LB(%webgwname,%webgwaccess,%webgwactive)
 if webgwnew=1 {
   for i=1:1 if '$D(@CtlGlo@("webgw","d",i)) quit
   set webgwid=i
