 elseif $G(itemid)="webgw.edit.existed" { 
   set webgwnew=0
   for { 
     R "webgwid>",webgwid W !
     if webgwid="" goto WebGWConfig
     if $D(@CtlGlo@("webgw","d",webgwid),webgwrec) {  set $LB(%webgwname,%webgwaccess,%webgwactive)=webgwrec quit }
   }
